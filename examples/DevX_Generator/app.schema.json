						"$id": "#root/bindings/gpio_inputs/items", 
						"title": "Items", 
						"type": "object",
						"required": [
							"name",
							"enabled",
							"properties"
						],
						"properties": {
							"name": {
								"$id": "#root/bindings/gpio_inputs/items/name", 
