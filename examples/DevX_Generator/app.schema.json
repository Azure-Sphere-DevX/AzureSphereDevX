									"PublishTelemetry", "Watchdog"
								],
								"pattern": "^.*$"
							},
							"enabled": {
								"$id": "#root/bindings/general/items/enabled", 
								"title": "Enabled", 
								"type": "boolean",
								"examples": [
									false
								],
								"default": true
							},
							"properties": {
								"$id": "#root/bindings/general/items/properties", 
								"title": "Properties", 
								"type": "object",
								"required": [
									"period"
