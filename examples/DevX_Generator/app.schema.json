						"$id": "#root/bindings/general/items", 
						"title": "Items", 
						"type": "object",
						"required": [
							"name",
							"enabled",
							"properties",
							"handler_template"
						],
						"properties": {
							"name": {
								"$id": "#root/bindings/general/items/name", 
								"title": "Name", 
								"type": "string",
								"default": "",
