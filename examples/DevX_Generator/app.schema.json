	],
	"properties": {
		"bindings": {
			"$id": "#root/bindings", 
			"title": "Bindings", 
			"type": "object",
			"properties": {
				"device_twins": {
					"$id": "#root/bindings/device_twins", 
					"title": "Device_twins", 
					"type": "array",
					"default": [],
					"items":{
						"$id": "#root/bindings/device_twins/items", 
						"title": "Items", 
						"type": "object",
						"required": [
							"name",
							"properties"
						],
						"properties": {
							"name": {
								"$id": "#root/bindings/device_twins/items/name", 
								"title": "Name", 
								"type": "string",
								"default": "",
								"examples": [
									"DesiredHumidity", "DesiredTemperature", "ReportedTemperature"
								],
								"pattern": "^.*$"
							},
							"enabled": {
								"$id": "#root/bindings/device_twins/items/enabled", 
								"title": "Enabled", 
								"type": "boolean",
								"examples": [
									true
								],
								"default": true
							},
							"properties": {
								"$id": "#root/bindings/device_twins/items/properties", 
								"title": "Properties", 
								"type": "object",
								"required": [
									"type"
								],
								"properties": {
									"cloud2device": {
										"$id": "#root/bindings/device_twins/items/properties/cloud2device", 
										"title": "Cloud2device", 
										"type": "boolean",
										"examples": [
											true, false
										],
										"default": true
									},
									"type": {
										"$id": "#root/bindings/device_twins/items/properties/type", 
										"title": "Type", 
										"type": "string",
										"default": "",
										"examples": [
											"float", "double", "integer", "boolean", "string"
										],
										"pattern": "^.*$"
									}
								}
							}

						}
					}

				},
				"direct_methods": {
					"$id": "#root/bindings/direct_methods", 
					"title": "Direct_methods", 
					"type": "array",
					"default": [],
					"items":{
						"$id": "#root/bindings/direct_methods/items", 
						"title": "Items", 
						"type": "object",
						"required": [
							"name",
							"enabled"
						],
						"properties": {
							"name": {
								"$id": "#root/bindings/direct_methods/items/name", 
								"title": "Name", 
								"type": "string",
								"default": "",
								"examples": [
									"LightControl", "FanOn", "FanOff"
								],
								"pattern": "^.*$"
							},
							"enabled": {
								"$id": "#root/bindings/direct_methods/items/enabled", 
								"title": "Enabled", 
								"type": "boolean",
								"examples": [
									false
								],
								"default": true
							}
						}
					}

				},
				"timers": {
					"$id": "#root/bindings/timers", 
					"title": "Timers", 
					"type": "array",
					"default": [],
					"items":{
						"$id": "#root/bindings/timers/items", 
						"title": "Items", 
						"type": "object",
						"required": [
							"name",
							"enabled",
							"properties"
						],
						"properties": {
							"name": {
								"$id": "#root/bindings/timers/items/name", 
								"title": "Name", 
								"type": "string",
								"default": "",
								"examples": [
									"MeasureTemperature"
								],
								"pattern": "^.*$"
							},
							"enabled": {
								"$id": "#root/bindings/timers/items/enabled", 
								"title": "Enabled", 
								"type": "boolean",
								"examples": [
									false
								],
								"default": true
							},
							"properties": {
								"$id": "#root/bindings/timers/items/properties", 
								"title": "Properties", 
								"type": "object",
								"required": [
									"period",
									"type"
								],
								"properties": {
									"period": {
										"$id": "#root/bindings/timers/items/properties/period", 
										"title": "Period", 
										"type": "string",
										"default": "",
										"examples": [
											"30, 0", "15, 0", "5, 0", "0, 200 * ONE_MS", "0, 500000000"
										],
										"pattern": "^.*$"
									},
									"type": {
										"$id": "#root/bindings/timers/items/properties/type", 
										"title": "Type", 
										"type": "string",
										"default": "periodic",
										"examples": [
											"periodic", "oneshot"
										],
										"pattern": "^.*$"
									}
								}
							}

						}
					}

				},
				"gpio_outputs": {
					"$id": "#root/bindings/gpio_outputs", 
